/*
https://practice.geeksforgeeks.org/problems/detect-loop-in-linked-list/1/

Correct Answer.Correct Answer
Execution Time:1.11
*/
class Solution {
    public static boolean detectLoop(Node head){
        Node fast = head;
        Node slow = head;
        while(fast!=null && fast.next!=null && slow!=null){
            fast=fast.next.next;
            slow = slow.next;
            if(fast==slow){
                return true;
            }
        }
        return false;
    }
}
