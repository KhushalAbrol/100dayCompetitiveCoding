//https://practice.geeksforgeeks.org/problems/index-of-an-extra-element/1

//Correct Answer.Correct Answer
//Execution Time:1.77

class GfG {
    public int findExtra(int a[], int b[], int n) {
        for(int i=0;i<n-1;i++){
            if(a[i]!=b[i]){
                return i;
            }
        }
        return n-1;
    }
}
