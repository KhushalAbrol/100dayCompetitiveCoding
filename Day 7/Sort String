import java.util.*;
import java.lang.*;
import java.io.*;
class GFG
 {
	public static void main (String[] args)
	 {
	    Scanner sc = new Scanner(System.in);
	    int t = sc.nextInt();
	    sc.nextLine();
	    while(t-->0){
	        String str = sc.nextLine();
	        char[] ch = str.toCharArray();
	        
	        Arrays.sort(ch);
	        
	        for(int i=0;i<ch.length;i++){
	            System.out.print(ch[i]);
	        }
	         System.out.println();
	    }
	 }
}
